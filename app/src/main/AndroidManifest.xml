<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstmobilebasicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirstMobileBasicApp">
        <activity android:name=".UpdateCandidatActivity"></activity>
        <activity android:name=".ShowCandidatActivity" />
        <activity android:name=".RegisterByUserActivity" />
        <activity android:name=".RegisterByAdminActivity" />
        <activity android:name=".SuccessRegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>